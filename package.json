    "flux",
    "mobx",
    "observable",
    "rxjs",
    "angular",
    "@angular"
  ],
  "dependencies": {
    "spawn-x": "^2.3.5"
  },
  "devDependencies": {
    "husky": "^0.13.2",
    "ts-loader": "^2.0.1",
    "typescript": "^2.2.1",
    "webpack": "2.2.0"
  },
  "peerDependencies": {
    "@angular/core": "^2.4.9",
    "rxjs": "^5.2.0"
  }
